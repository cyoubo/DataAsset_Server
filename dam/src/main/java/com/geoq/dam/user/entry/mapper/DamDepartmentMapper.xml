<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.geoq.ex.mybatis.bean.DamDepartmentMapper">
  <resultMap id="BaseResultMap" type="com.geoq.ex.mybatis.bean.DamDepartment">
    <constructor>
      <arg column="uuid" javaType="java.lang.String" jdbcType="VARCHAR" />
      <arg column="departmentName" javaType="java.lang.String" jdbcType="VARCHAR" />
    </constructor>
  </resultMap>
  <insert id="insert" parameterType="com.geoq.ex.mybatis.bean.DamDepartment">
    insert into dam_department (uuid, departmentName)
    values (#{uuid,jdbcType=VARCHAR}, #{departmentname,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.geoq.ex.mybatis.bean.DamDepartment">
    insert into dam_department
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="uuid != null">
        uuid,
      </if>
      <if test="departmentname != null">
        departmentName,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="uuid != null">
        #{uuid,jdbcType=VARCHAR},
      </if>
      <if test="departmentname != null">
        #{departmentname,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>